Algoritmo Edad_Peso
	
	// Definir Variables 
	Definir Edad Como Entero;
	Definir Peso Como Entero;
	
	// Datos de Entrada 
	Escribir "Ingrese su edad";
	Leer Edad;
	Escribir "Ingrese su peso";
	Leer Peso;
	
	// Datos de Salida 
	Escribir "Su peso es: ",Peso," kg. ","Y su edad es: ",Edad," años.";
	
FinAlgoritmo
